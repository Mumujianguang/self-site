#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/bin/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/node_modules:/Users/linjianguang/code/self-site/node_modules:/Users/linjianguang/code/node_modules:/Users/linjianguang/node_modules:/Users/node_modules:/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+vite@3.1.2_less@4.1.3/node_modules/vite/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+vite@3.1.2_less@4.1.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/bin/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/node_modules:/Users/linjianguang/code/self-site/node_modules:/Users/linjianguang/code/node_modules:/Users/linjianguang/node_modules:/Users/node_modules:/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+vite@3.1.2_less@4.1.3/node_modules/vite/node_modules:/Users/linjianguang/code/self-site/node_modules/.pnpm/registry.npmmirror.com+vite@3.1.2_less@4.1.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+less@4.1.3/node_modules/less/bin/lessc" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+less@4.1.3/node_modules/less/bin/lessc" "$@"
fi
